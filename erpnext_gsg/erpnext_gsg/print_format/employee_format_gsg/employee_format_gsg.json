 "html": "<!DOCTYPE html>\n<html>\n<head>\n<title>Page Title</title>\n</head>\n<body>\n\n<div style=\"width:90%;margin:0 auto;border:1px solid gray;\"> <!-- big container -->\n\n<div style=\"width:100%;height:200px;float:left;background-color:gray;\"> <!-- first part -->\n\n<div style=\"align-items: center; text-align: center; margin:0 auto;\">\n     <h1 style=\"color:#fff;margin-top:17%;\"><strong>{{ doc.company }}</strong></h1>\n</div>\n       \n</div><!-- first part -->\n<div style=\"width:100%;height:100px;float:left;background-color:#00b7ff\">\n    <div  class=\"center\">\n        <img class=\"profile_photo\" src=\"{{ doc.image }}\" style=\"width:200px;height:200px; border-redius:50%;\" />\n    </div> <!--photo-->\n</div><!--second part -->\n<div style=\"width:100%;height:100px;float:left;background-color:#fff\"></div>\n<div class=\"body_content\">\n    <div style=\"width:100%;float:left;\">\n        <div style=\"align-items: center;text-align: center;margin:0 auto;\">\n        <h1 style=\"color:#aaa;text-align:center\">{{ doc.first_name }}</h1>\n        <h1 style=\"color:#888282;text-align:center\"><strong>{{ doc.last_name }}</strong></h1>\n        </div>\n        <div style=\"align-items: center;text-align: center;margin:0 auto;\">\n        <h1 style=\"color:#90C4F2;text-align:center;font-weight: lighter;margin-bottom:30px;\">{{ doc.department }}</h1>\n        </div>\n    </div>\n    \n    <table style=\"width:65% ;margin:0 auto; padding-top:20px;\">\n\t<tbody>\n\t\t\t<tr>\n\t\t\t<td style=\"color:#90C4F2\">Status  :</td>\n\t\t\t<td style=\"\">{{doc.status}} </td>\n\t\t</tr>\n\t\t\t<tr>\n\t\t\t<td style=\"color:#90C4F2\">Gender  :</td>\n\t\t\t<td style=\"\">{{doc.gender}} </td>\n\t\t</tr>\n\t\t<tr>\n\t\t\t<td style=\"color:#90C4F2\">DOB  :</td>\n\t\t\t<td style=\"\">{{doc.date_of_birth}} </td>\n\t\t</tr>\n\t\t<tr>\n\t\t\t<td style=\"color:#90C4F2\">Phone  :</td>\n\t\t\t<td style=\"\">{{doc.cell_number}} </td>\n\t\t</tr>\n\t\t<tr>\n\t\t\t<td style=\"color:#90C4F2\">Email  :</td>\n\t\t\t<td style=\"\">{{doc.prefered_email}} </td>\n\t\t</tr>\n\t\t<tr>\n\t\t\t<td style=\"color:#90C4F2\">Join  :</td>\n\t\t\t<td style=\"\">{{doc.date_of_joining}} </td>\n\t\t</tr>\n\t</tbody>\n</table>\n</div><!-- third part -->\n<div style=\"width:100%;height:80px;float:left;background-color:gray\">\n    <ul>\n        <li><img class=\"social\" src=\"/private/files/socialmedia.png\" /></li>\n       <li><img class=\"social\" src=\"/private/files/socialmedia(1).png\" /></li>\n        <li><img class=\"social\" src=\"/private/files/socialmedia(2).png\" /></li>\n    </ul>\n    <div style=\"width:40%;float:right;margin-right:2%;\"><h1 style=\"color:#fff;\">/ brandname</h1> </div>\n</div>\n<div style=\"width:100%;height:60px;float:left;background-color:#00b7ff\"></div>\n</div> <!-- big container -->\n</body>\n</html> <!---->",
